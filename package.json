{
  "name": "@zuchaowang/ip-hilbert",
  "version": "0.0.1",
  "description": "A library to map IPv4 and IPv6 to 2D space via Hilbert curve",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-debug": "node --inspect-brk -r ts-node/register ./node_modules/jest/bin/jest.js",
    "check": "eslint src/**/*.ts demosrc/**/*.ts && tsc --noEmit",
    "start": "webpack serve --mode development",
    "build-demo": "shx rm -rf demo && webpack --mode production",
    "build-lib": "shx rm -rf dist && tsc --project tsconfig.build.json",
    "build-docs": "typedoc",
    "build": "npm run check && npm run build-demo && npm run build-lib && npm run build-docs",
    "clean": "shx rm -rf demo && shx rm -rf dist && shx rm -rf docs"

  },
  "keywords": [
    "ipv4",
    "ipv6",
    "hilbert curve"
  ],
  "author": "zuchao.wang",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@typescript-eslint/typescript-estree": "5.60.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "shx": "^0.3.4",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ]
}
